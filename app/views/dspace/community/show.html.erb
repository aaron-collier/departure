<div id="page-wrapper">
     <div class="row">
         <div class="col-lg-12">
             <h1 class="page-header"><%= @community.name %></h1>
             <ol class="breadcrumb">
               <li class="breadcrumb-item"><a href="/">Home</a></li>
               <li class="breadcrumb-item"><a href="/communities">Dspace Communities</a></li>
               <li class="breadcrumb-item active"><a href="#"><%= @community.name %></a></li>
             </ol>
         </div>
         <!-- /.col-lg-12 -->
     </div>
     <!-- /.row -->
     <div class="row">

       <!-- Metadata Fields in Use Panel -->
       <div class="col-lg-8">
         <div class="panel panel-default">
           <div class="panel-heading">
             <i class="fa fa-bars" aria-hidden="true"></i>
             Metadata Fields In Use
           </div>
           <div class="panel-body">
             <ul>
             <% @comm_info.metadata_fields.each do |field| %>
               <li>
                 <% if field.qualifier.nil? %>
                 <%= link_to 'dc.'+field.element, dspace_community_metadata_path(field) %>
                 <% else %>
                 <%= link_to 'dc.'+field.element+'.'+field.qualifier, dspace_community_metadata_path(field) %>
                 <% end %>
                </li>
             <% end %>
             </ul>
           </div>
         </div>
       </div>

       <!-- Metadata Field data Panel -->
       <div class="col-lg-8">
         <div class="panel panel-default">
           <div class="panel-heading">
             <i class="fa fa-sun-o" aria-hidden="true"></i>
             Value list
           </div>
           <div class="panel-body">
           </div>
         </div>
       </div>

     </div>
</div>

<script>
jQuery(document).ready(function($) {
    $(".clickable-row").click(function() {
        window.location = $(this).data("url");
    });
});
</script>